#include <iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    //задача1
    cout << "\n1. Делители чисел от 10 до 15:" << endl;
    for (int num = 10; num <= 15; num++) { //Перебираем числа от 10 до 15
        cout << "Число " << num << ": ";
        for (int i = 1; i <= num; i++) { //проверяем все возможные делители
            if (num % i == 0) { //если делится без остатка
                cout << i << " "; //выводим делитель
            }
        }
        cout << endl;
    }

    //задача 2
    cout << "\n2. Первые 20 простых чисел:" << endl;
    int count = 0;
    for (int num = 2; count < 20; num++) {//ищем пока не найдем 20 простых чисел
        bool prostoe = true;
        for (int i = 2; i * i <= num; i++) {//проверяем делители до корня числа
            if (num % i == 0) {
                prostoe = false; //число не простое
                break;
            }
        }
        if (prostoe) {  //если число простое
            cout << num << " "; //Выводим его
            count++; //увеличиваем счетчик
        }
    }

    //задача 4
    cout << "\n\n4. Комбинации кода чемодана:" << endl;
    int comb = 0;//счетчик комбинаций
    for (int i = 0; i <= 9; i++) {  // первая цифра кода
        for (int j = 0; j <= 9; j++) { //вторая цифра кода
            for (int k = 0; k <= 9; k++) { // третья цифра кода
                if (i != j && i != k && j != k) {// проверяем что все цифры разные
                    comb++;
                }
            }
        }
    }
    cout << "Всего комбинаций: " << comb << endl;
    cout << "Время на перебор: " << comb * 3 / 60 << " минут" << endl; // 

    //задача 5
    cout << "\n5. Зарплата за квартал:" << endl;
    int kv[3][3] = {
        {50000, 52000, 53000},// зп первого сотрудника
        {48000, 49000, 50000},//зп второго сотрудника
        {55000, 56000, 57000} //зп третьего сотрудника
    };

    int total = 0;
    for (int i = 0; i < 3; i++) { //перебираем сотрудников
        int sum = kv[i][0] + kv[i][1] + kv[i][2];//сумма зп за 3 месяца
        cout << "Сотрудник " << i + 1 << ": " << sum << " руб." << endl;
        total += sum;
    }
    cout << "Общая сумма: " << total << " руб." << endl;

    return 0;
}
